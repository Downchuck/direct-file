import '../compnodes';
import { compNodeRegistry } from '../compnodes/registry';
import { Result } from '../types';
import { Factual } from '../Factual';
import { Expression } from '../Expression';
import { FactDictionary } from '../FactDictionary';

describe('IpPinNode', () => {
  const factual = new Factual(new FactDictionary());

  it('can be created with a valid IP PIN', () => {
    const node = compNodeRegistry.fromDerivedConfig(
      {
        typeName: 'IpPin',
        children: [
          { typeName: 'String', value: '123456' },
        ],
      },
      factual.graph
    );
    expect(node.get(factual)).toEqual(Result.complete('123456'));
  });

  it('is incomplete with an invalid IP PIN', () => {
    const node = compNodeRegistry.fromDerivedConfig(
      {
        typeName: 'IpPin',
        children: [
          { typeName: 'String', value: '12345' },
        ],
      },
      factual.graph
    );
    expect(node.get(factual).isComplete).toBe(false);
  });
});
